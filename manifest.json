{
  "update_url":"http://clients2.google.com/service/update2/crx",
  "minimum_chrome_version": "22",
  "offline_enabled": true,
  "default_locale": "en",
  "background": {
    "scripts": [
      "/javascript/jquery.js",
      "/javascript/background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides" : {
    "newtab": "ntp.html"
  },
  "options_page": "options.html",
  "name": "Awesome New Tab Page",
  "description": "Enhance your New Tab Page with ultimate customizability and power. Awesome, like you. [ANTP]",
  "version": "2013.103.6.0",
  "icons": {
    "128": "icon128.png"
  },
  "optional_permissions": [ "*://*/*" ],
  "permissions": [
    "management",
    "bookmarks",
    "tabs",
    "storage",
    "unlimitedStorage",
    "chrome://favicon/",
    "*://*.antp.co/*"
  ],
  "web_accessible_resources": [
    "ntp.html",
    "options.html",
    "/widgets/tabs/tabs.html",
    "/widgets/tv/widget.tv.html",
    "/widgets/google/widget.google.html",
    "/widgets/clock/widget.clock.html",
    "/widgets/tutorial/widget.tutorial.html",
    "/widgets/notepad/widget.notepad.html"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://apis.google.com https://ssl.google-analytics.com https://platform.twitter.com https://api.flattr.com; object-src 'self'"
}
